    "import RPi.GPIO as GPIO\n",
    "from mfrc522 import SimpleMFRC522\n",
    "\n",
    "reader = SimpleMFRC522()\n",
    "\n",
    "try:\n",
    "    id, text= reader.read()\n",
    "    print(id)\n",
    "    print(text)\n",
    "\n",
    "finally:\n",
    "    GPIO.cleanup()"
